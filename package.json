{
  "name": "axios-seer",
  "version": "1.0.1",
  "description": "基于axios封装的请求库",
  "type": "module",
  "keywords": [
    "axios"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/axios-seer.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "commitlint": "commitlint --edit",
    "prepare": "simple-git-hooks",
    "lint": "tsc --noEmit && eslint . --fix",
    "up": "taze -I -w",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "bump": "bumpp package.json -c --no-push -t --all -x \"npm run changelog\""
  },
  "peerDependencies": {
    "axios": ">=1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@minko-fe/commitlint-config": "^2.1.2",
    "@minko-fe/eslint-config": "^4.0.2",
    "@minko-fe/prettier-config": "^2.2.3",
    "@minko-fe/tsconfig": "^2.1.1",
    "@types/clone-deep": "^4.0.4",
    "@types/lodash.omit": "^4.5.9",
    "axios": "^1.7.7",
    "bumpp": "^9.5.2",
    "clone-deep": "^4.0.1",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "eslint": "^9.10.0",
    "lodash.omit": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "query-string": "8.1.0",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.8",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "simple-git-hooks": {
    "commit-msg": "npm run commitlint",
    "pre-commit": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@minko-fe/commitlint-config"
    ]
  },
  "prettier": "@minko-fe/prettier-config"
}
